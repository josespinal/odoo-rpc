{
    "name": "josespinal/odoo-rpc",
    "description": "PHP Odoo RPC connector with JSON-RPC and XML-RPC support, prepared for Laravel integration",
    "keywords": [
        "php",
        "odoo",
        "jsonrpc",
        "xmlrpc",
        "laravel",
        "erp"
    ],
    "homepage": "https://github.com/josespinal/odoo-rpc",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jose Espinal",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "phpxmlrpc/phpxmlrpc": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Obuchmann\\OdooJsonRpc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Obuchmann\\OdooJsonRpc\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Obuchmann\\OdooJsonRpc\\OdooServiceProvider"
            ]
        }
    }

}
